{
  "name": "nuxt-template",
  "version": "1.0.0",
  "description": "My laudable Nuxt.js project",
  "author": "Estuardo Aguirre",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
